   "source": [
    "# count_vowels.py\n",
    "\n",
    "text = \"Python is awesome\"\n",
    "vowels = \"aeiouAEIOU\"\n",
    "count = 0\n",
    "\n",
    "for char in text:\n",
    "    if char in vowels:\n",
    "        count += 1\n",
    "\n",
    "print(\"모음 개수 :\", count)\n"
   ]
