    "# email_validator.py\n",
    "\n",
    "# 이메일 리스트 (예시)\n",
    "emails = [\"user@example.com\", \"user@example\"]\n",
    "\n",
    "for email in emails:\n",
    "    print(f\"이메일 주소: {email}\")\n",
    "    if \"@\" in email and \".\" in email:\n",
    "        print(\"유효함\\n\")\n",
    "    else:\n",
    "        print(\"유효하지 않음\\n\")\n"
   ]
  }
