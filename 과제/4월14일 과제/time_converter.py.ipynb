   "source": [
    "# time_converter.py\n",
    "\n",
    "total_seconds = 12345\n",
    "\n",
    "# 시간 계산\n",
    "hours = total_seconds // 3600\n",
    "remaining_seconds = total_seconds % 3600\n",
    "\n",
    "# 분과 초 계산\n",
    "minutes = remaining_seconds // 60\n",
    "seconds = remaining_seconds % 60\n",
    "\n",
    "# 결과 출력\n",
    "print(f\"{total_seconds}초는 {hours}시간 {minutes}분 {seconds}초입니다.\")\n"
   ]
  }
